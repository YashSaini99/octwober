{
    "compilerOptions": {
        "lib": ["ES2020", "esnext", "esnext.array", "esnext.asynciterable", "esnext.intl", "esnext.symbol"],
        "pretty": true,
        "strict": true,
        "outDir": "dist",
        "rootDir": "./",
        "sourceMap": true,
        "target": "ES2020",
        "module": "CommonJS",
        "declaration": false,
        "alwaysStrict": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "removeComments": false,
        "resolveJsonModule": true,
        "moduleResolution": "node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "allowSyntheticDefaultImports": true,
        "forceConsistentCasingInFileNames": true,
        "baseUrl": "src",
        "paths": {
            "#utils/*": ["bot/utils/*"],
			"#struct/*": ["bot/struct/*"],
            "#bot/*": ["bot/*"],
			"#root/*": ["*"],
        },
        "typeRoots": ["./node_modules/@types", "./src/types"]
    }
}